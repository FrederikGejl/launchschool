let array1 = [1, 2, undefined, 4]; /// 4

array1.length;

let array2 = [1]; /// 5
array2.length = 5;

let array3 = []; ///// 0
array3[-1] = [1];

let array4 = [1, 2, 3, 4, 5]; /// 3
array4.length = 3;

let array5 = []; //// 101
array5[100] = 3;


let myArray = [1, 3, 6, 11, 4, 2,
               4, 9, 17, 16, 0];

let evenArray = [];

myArray.forEach(function(num) {

  if (num % 2 === 0) {
evenArray.push(num)

  }

});

console.log(myArray);
console.log(evenArray)