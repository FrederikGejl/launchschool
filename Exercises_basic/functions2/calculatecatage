catAge(0); // 0
calculateCatYears(1); // 15
catAge(2); // 24
catAge(3); // 28
catAge(4); // 32



function calculateCatYears(humanYears) {
    let catYears = 0;

    switch (humanYears) {
        case 1:
            catYears += 15;
            break;
        case 2:
            catYears += 15 + 9;
            break;
        default:
            catYears += 15 + 9 + (humanYears - 2) * 4;
            break;
    }

    return catYears;
}


console.log(calculateCatYears(1)); // 15
console.log(calculateCatYears(5)); // Output: 36
console.log(calculateCatYears(34)); // Output: 36

// Example usage:
console.log(calculateCatYears(1)); // Output: 15
console.log(calculateCatYears(2)); // Output: 24
console.log(calculateCatYears(3)); // Output: 28
console.log(calculateCatYears(4)); // Output: 32
console.log(calculateCatYears(5)); // Output: 36





